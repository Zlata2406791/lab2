import math
r = float(input('Введите радиус круга'))
a = float(input('Введите координату x центра круга'))
b = float(input('Введите координату y центра круга'))
n = int(input('Введите количество точек'))
cnt = 0
for i in range(n):
    x = float(input('Введите координату x точки'))
    y = float(input('Введите координату y точки'))
    t = math.sqrt((x-a)**2+(y-b)**2)
    if t<=r:
        cnt += 1
print('Количество точек:', cnt)
