import math
def circle(x, y, a, b, r):
    d = (x-a)**2+(y-b)**2
    return d <= r**2
a = float(input('Введите координату Х'))
b = float(input('Введите координату Y'))
r = float(input('Введите радиус'))
count = 0
while True:
    try:
        x = float(input('Введите координату Х точки или букву для выхода'))
        y = float(input('Введите координату Y точки или букву для выхода'))
        if circle(x, y, a, b, r) == True:
            count += 1
    except:
        print('Выход из программы')
        break
print(count)
